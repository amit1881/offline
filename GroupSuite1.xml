<?xml version="1.0" encoding="UTF-8"?>
<suite name="suite ---inclusion of groups">
	<test name="grouping of tests">
		<groups>
			<define name="public_private">
				<include name="public" />
				<include name="private" />
			</define>
			<run>
				<include name="public_private" />
			</run>
		</groups>
		<classes>
			<class name="com.offline.TestGroup" />
		</classes>
	</test>
</suite>